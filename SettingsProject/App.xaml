<Application x:Class="SettingsProject.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SettingsProject"
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>

            <DataTemplate x:Key="UnconfiguredStringSettingValueTemplate" DataType="local:UnconfiguredSettingValue">
                <TextBox Text="{Binding Value, UpdateSourceTrigger=PropertyChanged}" TextWrapping="Wrap" />
            </DataTemplate>

            <DataTemplate x:Key="ConfiguredStringSettingValueTemplate" DataType="local:ConfiguredSettingValue">
                <DockPanel Margin="0,2">
                    <TextBlock DockPanel.Dock="Top" Text="{Binding Configuration}" TextWrapping="Wrap" />
                    <TextBox Text="{Binding Value, UpdateSourceTrigger=PropertyChanged}" TextWrapping="Wrap" />
                </DockPanel>
            </DataTemplate>

            <DataTemplate x:Key="UnconfiguredMultilineStringSettingValueTemplate" DataType="local:UnconfiguredSettingValue">
                <TextBox Text="{Binding Value, UpdateSourceTrigger=PropertyChanged}" AcceptsReturn="True" TextWrapping="NoWrap" MinHeight="42" MaxHeight="200" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" />
            </DataTemplate>

            <DataTemplate x:Key="ConfiguredMultilineStringSettingValueTemplate" DataType="local:ConfiguredSettingValue">
                <DockPanel Margin="0,2">
                    <TextBlock DockPanel.Dock="Top" Text="{Binding Configuration}" TextWrapping="Wrap" />
                    <TextBox Text="{Binding Value, UpdateSourceTrigger=PropertyChanged}" AcceptsReturn="True" TextWrapping="NoWrap" MinHeight="42" MaxHeight="200" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto" />
                </DockPanel>
            </DataTemplate>

            <DataTemplate x:Key="UnconfiguredBoolSettingValueTemplate" DataType="local:UnconfiguredSettingValue">
                <CheckBox IsChecked="{Binding Value}">
                    <local:HighlightTextBlock HighlightText="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=local:SettingsList}, Path=SearchViewModel.SearchString}" Text="{Binding Parent.Description}" TextWrapping="Wrap" />
                </CheckBox>
            </DataTemplate>

            <DataTemplate x:Key="ConfiguredBoolSettingValueTemplate" DataType="local:ConfiguredSettingValue">
                <CheckBox Margin="0,2" IsChecked="{Binding Value}">
                    <TextBlock Text="{Binding Configuration}" TextWrapping="Wrap" />
                </CheckBox>
            </DataTemplate>

            <DataTemplate x:Key="UnconfiguredEnumSettingValueTemplate" DataType="local:UnconfiguredSettingValue">
                <ComboBox ItemsSource="{Binding Parent.EnumValues}" SelectedItem="{Binding Value}" />
            </DataTemplate>

            <DataTemplate x:Key="ConfiguredEnumSettingValueTemplate" DataType="local:ConfiguredSettingValue">
                <DockPanel Margin="0,2">
                    <TextBlock DockPanel.Dock="Top" Text="{Binding Configuration}" TextWrapping="Wrap" />
                    <ComboBox ItemsSource="{Binding Parent.EnumValues}" SelectedItem="{Binding Value}" />
                </DockPanel>
            </DataTemplate>

            <!-- Setting data templates -->

            <DataTemplate x:Key="LinkActionTemplate" DataType="{x:Type local:Setting}">
              <DockPanel Margin="0,0,0,24" Background="Transparent">
                <local:HighlightTextBlock
                  DockPanel.Dock="Top"
                  Margin="28,0,0,4"
                  HighlightText="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=local:SettingsList}, Path=SearchViewModel.SearchString}"
                  Text="{Binding Converter={x:Static local:Converters.LinkActionHeading}}"
                  FontWeight="Bold"
                  Visibility="{Binding HasDescription, Converter={x:Static local:Converters.CollapsedWhenFalse}}" />
                <TextBlock Margin="28,0,0,6">
                  <Hyperlink>
                    <Run Text="{Binding Converter={x:Static local:Converters.LinkActionLinkText}, Mode=OneTime}" />
                  </Hyperlink>
                </TextBlock>
              </DockPanel>
            </DataTemplate>

            <DataTemplate x:Key="GenericSettingTemplate" DataType="local:Setting">
              <DockPanel Margin="0,0,0,24" Background="Transparent">
                <ContentControl DockPanel.Dock="Left" Width="16" IsTabStop="False" Focusable="False">
                  <TextBlock Margin="0,2,0,0" Name="CommandMenuButton" FontFamily="Segoe MDL2 Assets" FontSize="12" TextAlignment="Left" Text="&#xE713;" VerticalAlignment="Top" HorizontalAlignment="Left" Foreground="#888" Visibility="{Binding SupportsPerConfigurationValues, Converter={x:Static local:Converters.CollapsedWhenFalse}}" ContextMenuService.Placement="Bottom">
                    <TextBlock.ContextMenu>
                      <ContextMenu Name="ContextMenu" Placement="Bottom" VerticalOffset="4">
                        <!-- Command="{Binding ElementName=PART_ItemsControl, Path=UseSameValueAcrossConfigurationsCommand}" -->
                        <MenuItem
                            Header="Use single value across configurations"
                            CommandParameter="{Binding}"
                            IsChecked="{Binding Path=HasPerConfigurationValues, Mode=OneWay, Converter={x:Static local:Converters.Negate}}" />
                        <!-- Command="{Binding ElementName=PART_ItemsControl, Path=UseDifferentValuesAcrossConfigurationsCommand}" -->
                        <MenuItem
                            Header="Specify value per configuration"
                            CommandParameter="{Binding}"
                            IsChecked="{Binding Path=HasPerConfigurationValues, Mode=OneWay}" />
                      </ContextMenu>
                    </TextBlock.ContextMenu>
                  </TextBlock>
                </ContentControl>
                <local:HighlightTextBlock DockPanel.Dock="Top" Margin="12,0,0,4" HighlightText="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=local:SettingsList}, Path=SearchViewModel.SearchString}" Text="{Binding Name}" FontWeight="Bold" />
                <local:HighlightTextBlock DockPanel.Dock="Top" Margin="12,0,0,6" HighlightText="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType=local:SettingsList}, Path=SearchViewModel.SearchString}" Text="{Binding Description}" TextWrapping="Wrap" Visibility="{Binding HasDescription, Converter={x:Static local:Converters.CollapsedWhenFalse}}" />
                <ItemsControl ItemsSource="{Binding Values}" IsTabStop="False" Focusable="False">
                  <ItemsControl.ItemTemplate>
                    <DataTemplate DataType="local:ISettingValue">
                      <ContentPresenter Content="{Binding}" ContentTemplate="{Binding Path=Template}" Margin="12,0,0,0" />
                    </DataTemplate>
                  </ItemsControl.ItemTemplate>
                </ItemsControl>
              </DockPanel>
              <DataTemplate.Triggers>
                <Trigger Property="IsMouseOver" Value="False">
                  <Setter TargetName="CommandMenuButton" Property="Visibility" Value="Collapsed" />
                </Trigger>
                <DataTrigger Binding="{Binding ElementName=ContextMenu, Path=IsOpen}" Value="True">
                  <Setter TargetName="CommandMenuButton" Property="Visibility" Value="Visible" />
                </DataTrigger>
              </DataTemplate.Triggers>
            </DataTemplate>

            <!-- Icons -->

            <SolidColorBrush x:Key="canvas"  Opacity="0"/>
            <SolidColorBrush x:Key="light-defaultgrey" Color="#212121" Opacity="1"/>
            <SolidColorBrush x:Key="light-defaultgrey-10" Color="#212121" Opacity="0.1"/>
